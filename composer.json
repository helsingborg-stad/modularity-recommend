{
    "name": "helsingborg-stad/modularity-recommend",
    "description": "Modularity RekAI",
    "type": "wordpress-plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "Sebastian Thulin",
            "email": "sebastian.thulin@helsingborg.se"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "installer-paths": {
            "vendor/{$vendor}/{$name}": [
                "type:wordpress-muplugin",
                "type:wordpress-theme",
                "type:wordpress-plugin",
                "type:acf-plugin"
            ]
        }
    },
    "require": {
        "helsingborg-stad/municipio": "^6.0.0"
    },
    "autoload": {
        "psr-4": {
            "ModularityRecommend\\": "source/php/"
        }
    },
    "version": "4.0.1",
    "config": {
        "allow-plugins": {
            "composer/installers": true
        }
    }
}